{
  "name": "trading-bot",
  "version": "1.0.0",
  "description": "AI-Regulated Trading Bot with Binance API Integration",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "session:clean": "rm -rf sessions/*",
    "session:backup": "cp -r sessions/ backup-sessions-$(date +%Y%m%d)/",
    "session:list": "ls -la sessions/"
  },
  "keywords": [
    "trading",
    "bot",
    "binance",
    "ai",
    "cryptocurrency"
  ],
  "author": "Trading Bot Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "binance-api-node": "^0.12.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
